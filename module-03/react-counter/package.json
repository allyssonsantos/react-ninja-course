{
  "scripts": {
    "dev": "webpack-dashboard -- node server.js",
    "test": "jest --coverage",
    "test:watch": "yarn test -- --watch",
    "lint": "eslint ./src/** --ext .js --ext .jsx",
    "lint:fix": "semistandard --fix",
    "build": "cross-env NODE_ENV=production webpack --progress --config webpack/prod.config -p",
    "storybook": "start-storybook -p 6006 -c storybook",
    "build-storybook": "cross-env NODE_ENV=production build-storybook -c storybook",
    "update-packages": "node ./scripts/update.js"
  },
  "devDependencies": {
    "babel-core": "^6.22.1",
    "babel-eslint": "^8.2.2",
    "babel-loader": "^6.2.10",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.22.0",
    "babel-preset-stage-0": "^6.22.0",
    "chai": "^4.1.2",
    "clean-webpack-plugin": "^0.1.19",
    "cross-env": "^3.1.4",
    "css-loader": "^0.26.1",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-loader": "^2.0.0",
    "eslint-plugin-import": "^2.11.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "^7.7.0",
    "extract-text-webpack-plugin": "^2.0.0",
    "file-loader": "^1.1.11",
    "html-webpack-plugin": "2.28.0",
    "prop-types": "^15.6.1",
    "react-hot-loader": "3.0.0-beta.6",
    "snazzy": "^6.0.0",
    "style-loader": "^0.20.2",
    "url-loader": "^1.0.1",
    "webpack": "2.2.1",
    "webpack-dashboard": "0.3.0",
    "webpack-dev-server": "2.3.0"
  },
  "dependencies": {
    "babel-runtime": "^6.26.0",
    "eslint": "^4.19.1",
    "jest": "^22.4.3",
    "react": "^16.3.0",
    "react-dom": "^16.3.0",
    "react-test-renderer": "^16.3.1"
  },
  "semistandard": {
    "globals": [
      "alert"
    ],
    "ignore": [
      "dist",
      "node_modules"
    ]
  },
  "jest": {
    "moduleNameMapper": {
      "^.+\\.css$": "<rootDir>/config/CSSStub.js"
    }
  }
}
